{
  "name": "@sentry-internal/rrdom",
  "version": "2.31.0",
  "homepage": "https://github.com/rrweb-io/rrweb/tree/main/packages/rrdom#readme",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rrdom.cjs",
  "module": "./dist/rrdom.js",
  "unpkg": "./dist/rrdom.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rrdom.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/rrdom.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrweb-io/rrweb.git"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "yarn run check-types && vite build",
    "build:tarball": "npm pack",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepack": "yarn run build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "bugs": {
    "url": "https://github.com/rrweb-io/rrweb/issues"
  },
  "devDependencies": {
    "@sentry-internal/rrweb-types": "2.31.0",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "puppeteer": "^17.1.3",
    "typescript": "^4.9.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1"
  },
  "dependencies": {
    "@sentry-internal/rrweb-snapshot": "2.31.0"
  }
}
