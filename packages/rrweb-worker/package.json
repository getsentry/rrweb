{
  "name": "@sentry-internal/rrweb-worker",
  "version": "2.31.0",
  "description": "Worker for rrweb",
  "main": "dist/rrweb-worker/index.js",
  "module": "dist/rrweb-worker/index.mjs",
  "types": "dist/typings/index.d.ts",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build": "rollup --config",
    "typings": "tsc -p tsconfig.types.json",
    "prepare": "yarn run typings && yarn run build",
    "prepublish": "yarn prepare",
    "check-types": "tsc -noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:update": "vitest run --update",
    "lint": "yarn eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/rrweb.git"
  },
  "author": "Sentry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getsentry/rrweb/issues"
  },
  "dependencies": {
    "@sentry-internal/rrweb-snapshot": "2.31.0",
    "@sentry-internal/rrweb-types": "2.31.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/css-font-loading-module": "0.0.7",
    "rollup": "^4.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=12"
  }
}
