{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "prepare": {
      "dependsOn": ["^prepare"],
      "outputs": ["lib/**", "es/**", "dist/**", "typings/**"]
    },
    "test": {
      "dependsOn": ["^prepublish"]
    },
    "test:watch": {
      "persistent": true
    },
    "test:update": {},
    "dev": {
      "dependsOn": ["^prepare"],
      "persistent": true
    },
    "lint": {},
    "typings": {},
    "check-types": {}
  }
}
